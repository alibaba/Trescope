{
    "name": "trescope_frontend",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:9000",
    "dependencies": {
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-scripts": "3.4.0",
        "plotly.js": "file:../third/plotly.js",
        "@material-ui/core": "^4.2.1",
        "@material-ui/icons": "^4.2.1",
        "websocket": "^1.0.29",
        "react-resize-detector": "^4.2.0",
